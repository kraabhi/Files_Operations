import cv2
xmin=712
ymin=143
xmax=810
ymax=307
image=cv2.imread("000000.png") #sample image
cv2.rectangle(image,(xmin,ymin),(xmax,ymax),(0,0,255),2) # contains image name, coordinates, color of box(eg: 0,0,255 for red color), thickness of box
cv2.imshow('0000',image)
cv2.waitKey(0) #Press Enter to Exit
cv2.destroyAllWindows()
